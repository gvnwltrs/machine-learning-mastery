   "source": [
    "from numpy.random import multinomial\n",
    "\n",
    "p = [1.0/3.0, 1.0/3.0, 1.0/3.0]\n",
    "k = 100 \n",
    "\n",
    "cases = multinomial(k, p)\n",
    "\n",
    "for i in range(len(cases)): \n",
    "    print('Case {0}: {1}'.format(i+1, cases[i]))"
